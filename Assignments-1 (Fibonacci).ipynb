   "source": [
    "# Write a Python program to get the Fibonacci series between 0 to 50\n",
    "\n",
    "n = 50\n",
    "n1 = 0\n",
    "n2 = 1\n",
    "print(n2,end=\" \")\n",
    "for i in range (n2,n):\n",
    "    n3 = n1 + n2\n",
    "    if n3 <= n:\n",
    "        print (n3, end=\" \")\n",
    "    n1=n2\n",
    "    n2=n3"
   ]
